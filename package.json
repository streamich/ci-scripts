{
    "name": "ci-scripts",
    "version": "0.0.1",
    "bin": {
        "ci": "./bin/ci.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/streamich/ci-scripts.git"
      },
    "scripts": {
        "build:readme": "mmarkdown --src ./build/readme.md --out README.md --scripts ./build/mmd.js",
        "eslint": "eslint --ignore-path .gitignore './**/*.js'"
    },
    "dependencies": {
        "minimist": "^1.2.0",
        "request": "^2.85.0",
        "request-promise": "^4.2.2"
    },
    "devDependencies": {
        "glob": "^7.1.2",
        "mmarkdown": "^1.0.0",
        "eslint": "^4.19.1",
        "eslint-config-mailonline": "^9.0.0",
        "mol-conventional-changelog": "^1.2.0"
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/mol-conventional-changelog"
      }
    },
    "keywords": [
        "ci",
        "scripts",
        "ci-scripts",
        "travis",
        "circle",
        "travisci",
